public class TestDLinkedList {
	public static void main(String[] args) {
		DLinkedList d = new DLinkedList();
		d.insert(1); d.insert(5); d.insert(10);
		System.out.println(d);
		
		d.remove(); System.out.println(d);
		d.remove(); System.out.println(d);
		d.remove(); System.out.println(d);
		d.remove(); System.out.println(d);
		
		d.insert(1); d.insert(2); d.insert(3);
		System.out.println(d);
		
		d.remove(2); System.out.println(d);
		d.remove(5); System.out.println(d);
		
		d.insert(2); d.insert(3); d.insert(4);
		System.out.println(d);
		
		d.reverse();
		System.out.println(d);
	}
}
