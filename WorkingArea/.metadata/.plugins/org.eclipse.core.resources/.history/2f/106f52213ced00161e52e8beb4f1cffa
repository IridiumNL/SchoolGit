public class DLinkedTest {
	private DNode head;
	private DNode tail;
	int size;
	
	public DLinkedTest() {
		head = null;
		tail = null;
		size = 0;
	}
	
	public boolean isEmpty() {
		if(size == 0) {
			return true;
		} else {
			return false;
		}
	}
	
	public void insert(int data) {
		DNode n = new DNode(data);
		if (isEmpty()) {
			head = n;
		}
		if (size == 1) {
			// head (n)
			head.setNext(n);
			n.setPrev(head);
		} else {
			DNode tead = tail;
			tead.setNext(n);
			n.setPrev(tead);
		}
		
		tail = n;
		size++;
	}
}
